@model IEnumerable<Music.Models.Album>

@{
    ViewBag.Title = "artist";
}

<h2>Artist Albums</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Title)
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.ActionLink(item.Title, "Details", new { id = item.AlbumID })
            </td>
        </tr>
    }

</table>
<br>@Html.ActionLink("Back to List", "artist")